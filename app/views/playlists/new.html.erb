<h1>New Playlist</h1>

<%= form_for @playlist do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <% @songs.each do |song| %>
    <div>
      <%#= check_box_tag(:song_ids, song.id, false, name: song.title, id: song.id) %>
      <%= check_box_tag(:song_ids, song.id, false, :name => "playlist[song_ids][]", id: "song-#{song.id}") %>
      <%= song.title %>
    </div>
  <% end %>
  <%= f.submit %>
<% end %>
